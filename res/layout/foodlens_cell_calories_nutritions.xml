<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:background="@color/foodlens_white" android:layout_width="match_parent" android:layout_height="35dp">
    <RelativeLayout android:layout_width="match_parent" android:layout_height="35dp">
        <com.azumio.android.foodlenslibrary.views.XMLTypefaceTextView android:textSize="14sp" android:textColor="@color/foodlens_blue_color" android:gravity="center" android:id="@+id/cell_text_label" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_marginLeft="17dp" android:text="" android:singleLine="true" android:layout_alignParentLeft="true" android:layout_centerInParent="true" app:typeface="@string/font_path_roboto_regular"/>
        <com.azumio.android.foodlenslibrary.views.XMLTypefaceTextView android:textSize="14sp" android:textColor="@color/foodlens_blue_color" android:gravity="center" android:id="@+id/cell_text_value" android:background="@null" android:paddingRight="20dp" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_marginLeft="17dp" android:text="0 cal" android:singleLine="true" android:layout_alignParentRight="true" android:layout_centerInParent="true" app:typeface="@string/font_path_roboto_regular"/>

<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#fff"/>
        </shape>
    </item>
    <item android:top="-2dp" android:right="-2dp">
        <shape>
            <solid android:color="@android:color/transparent"/>
            <stroke android:width="0.5dp" android:color="#e3e3e3"/>
        </shape>
    </item>
    <item android:left="9dp" android:bottom="0.5dp">
        <shape android:shape="rectangle">
            <solid android:color="#fff"/>
        </shape>
    </item>
</layer-list>

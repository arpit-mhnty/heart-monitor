<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#ffffffff"/>
        </shape>
    </item>
    <item>
        <inset>
            <inset android:insetLeft="16dp" android:insetRight="0dp" android:insetTop="0dp" android:insetBottom="0dp">
                <shape android:shape="rectangle">
                    <solid android:color="#ffbfbfbf"/>
                </shape>
            </inset>
        </inset>
    </item>
</layer-list>
